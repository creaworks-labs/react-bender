{
  "name": "react-bender",
  "version": "0.3.1",
  "description": "A powerful style bender for React and React Native projects.",
  "author": "omerduzyol",
  "license": "MIT",
  "repository": "creaworks-labs/react-bender",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "lint:prettier-quite": "prettier 'src/**/*.{js,ts,tsx}' --write",
    "lint:prettier": "yarn lint:prettier-quite --list-different",
    "lint:ts": "tslint src/**/*.{ts,tsx}",
    "lint": "yarn lint:ts && yarn lint:prettier-quite",
    "build:bender": "rollup -c .rollup.bender.js",
    "build": "rm -rf dist && yarn build:bender --silent",
    "start": "yarn build:bender -w",
    "prepare": "yarn run build",
    "predeploy": "cd example-web && yarn install && yarn run build",
    "deploy": "gh-pages -d example-web/build",
    "pretest": "yarn lint && yarn rollup"
  },
  "dependencies": {
    "hoist-non-react-statics": "^3.2.1",
    "lodash": "^4.17.11",
    "postcss": "^7.0.6",
    "postcss-react-bender": "^0.2.3",
    "postcss-scss": "^2.0.0",
    "prop-types": "^15.6.2"
  },
  "peerDependencies": {
    "hoist-non-react-statics": "^3.2.1",
    "lodash": "^4.17.11",
    "postcss": "^7.0.6",
    "postcss-react-bender": "^0.2.3",
    "postcss-scss": "^2.0.0",
    "prop-types": "^15.6.2",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@types/jest": "^23.1.5",
    "@types/loader-utils": "^1.1.3",
    "@types/lodash": "^4.14.118",
    "@types/node": "^10.12.12",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "@types/react-is": "^16.5.0",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "cross-env": "^5.1.4",
    "gh-pages": "^1.2.0",
    "hoist-non-react-statics": "^3.2.1",
    "lodash": "^4.17.11",
    "postcss": "^7.0.6",
    "postcss-react-bender": "^0.2.3",
    "postcss-scss": "^2.0.0",
    "prettier": "^1.15.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-is": "^16.6.3",
    "react-scripts-ts": "^2.16.0",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-copy-glob": "^0.2.2",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-typescript2": "^0.17.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^2.8.3"
  },
  "files": [
    "dist"
  ]
}
